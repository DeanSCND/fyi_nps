-if @patient_count == 0
	%h1 New patient

	=form_for @patient, :html=> {:multipart=>true} do |f|
		= f.label :file
		%br
		= f.file_field :file
		%br
		= "For Run: " + @run.name
		= f.hidden_field :run, :value=>@run.id
		%br
		Create in Fluid
		= check_box_tag 'fluid'
		%br
		%br
		= f.submit "Upload" 
	= link_to 'Back', patients_path
-else
	%h2 There are already patients loaded for this survey. Is this survey live?