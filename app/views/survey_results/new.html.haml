%h1 Upload Sruvey Answers

=form_for @survey_result, :html=> {:multipart=>true} do |f|
	%span
		Choose a Fluid Survey file to load:
	= f.file_field :file, :class=>"btn btn-primary btn-xs"
	%br
	%br
	= f.submit "Upload", :class=>"btn btn-primary btn-sm" 
	= link_to 'Back', runs_path, :class=>"btn btn-default btn-sm"
